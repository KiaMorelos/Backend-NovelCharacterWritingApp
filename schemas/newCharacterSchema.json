{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "title": "Root Schema",
  "type": "object",
  "default": {},
  "required": ["name"],
  "additionalProperties": false,
  "properties": {
    "name": {
      "title": "character name Schema",
      "type": "string",
      "mininum": 1
    },
    "characterPhotoUrl": {
      "title": "The url Schema",
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "format": "url"
    }
  },
  "examples": [
    {
      "characterPhotoUrl": "https://www.aImage.com/character",
      "name": "Gabe Brenner"
    }
  ]
}
